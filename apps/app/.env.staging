# ======================
# ENVIRONMENT
# ======================
EXPO_PUBLIC_ENVIRONMENT=staging

# ======================
# API CONFIGURATION
# ======================
EXPO_PUBLIC_API_URL=https://staging-api.forest-focus.com/api
EXPO_PUBLIC_API_TIMEOUT=10000

# ======================
# APP CONFIGURATION
# ======================
EXPO_PUBLIC_APP_NAME=Forest Focus Staging
EXPO_PUBLIC_APP_VERSION=1.0.0

# ======================
# OBSERVABILITY
# ======================
# Sentry DSN (staging)
EXPO_PUBLIC_SENTRY_DSN=https://your-sentry-dsn@sentry.io/staging

# PostHog (staging)
EXPO_PUBLIC_POSTHOG_API_KEY=your-staging-posthog-key
EXPO_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# ======================
# FEATURE FLAGS
# ======================
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true

# ======================
# EAS BUILD
# ======================
EXPO_PUBLIC_EAS_PROJECT_ID=
